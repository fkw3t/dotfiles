<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
<dict>
    <key>name</key>
    <string>Lackluster</string>
    <key>settings</key>
    <array>
        <!-- Configurações gerais -->
        <dict>
            <key>settings</key>
            <dict>
                <key>background</key>
                <string>#000000</string> <!-- black -->
                <key>foreground</key>
                <string>#DDDDDD</string> <!-- gray9 -->
