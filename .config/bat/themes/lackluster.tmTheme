        </dict>

        <!-- Variável -->
        <dict>
            <key>scope</key>
            <string>variable</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#7a7a7a</string> <!-- var/var_member: gray6 -->
            </dict>
        </dict>

        <!-- Constante -->
        <dict>
            <key>scope</key>
            <string>constant</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#555555</string> <!-- const/const_builtin/tag/func_call/type/type_def/type_primitive: gray5 -->
            </dict>
        </dict>

        <!-- Função Definida -->
        <dict>
            <key>scope</key>
            <string>entity.name.function</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#708090</string> <!-- func_def/special: lack -->
            </dict>
        </dict>

        <!-- Built-in -->
        <dict>
            <key>scope</key>
            <string>support.function</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#444444</string> <!-- builtin: gray4 -->
            </dict>
        </dict>

        <!-- Parâmetro de Função -->
        <dict>
            <key>scope</key>
            <string>variable.parameter</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#7a7a7a</string> <!-- func_param: gray6 -->
            </dict>
        </dict>

        <!-- Keyword -->
        <dict>
            <key>scope</key>
            <string>keyword</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#555555</string> <!-- keyword: gray5 -->
            </dict>
        </dict>

        <!-- Keyword Return -->
        <dict>
            <key>scope</key>
            <string>keyword.control.return</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#789978</string> <!-- keyword_return: green -->
