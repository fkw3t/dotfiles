        </dict>

        <!-- Comentários -->
        <dict>
            <key>scope</key>
            <string>comment</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#deeeed</string> <!-- comment: luster -->
            </dict>
